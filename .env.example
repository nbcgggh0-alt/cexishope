# CexiStore Bot - Environment Variables
# Copy this file to .env and fill in your values

# Required: Telegram Bot Token from @BotFather
TELEGRAM_BOT_TOKEN=

# Required: Supabase project URL
SUPABASE_URL=

# Required: Supabase service_role key (NOT the anon key)
SUPABASE_KEY=

# Required: Owner Telegram ID (get from @userinfobot)
OWNER_ID=

# Optional: Store settings
STORE_NAME=CexiStore Ultimate Pro
STORE_CURRENCY=RM
SESSION_TIMEOUT=14400000
